<launch>
    <group ns="line_follower">
        <rosparam file="$(find line_follower)/config/line_follower_config.yaml" />
        <node name="line_follower_node" pkg="line_follower" type="line_follower_node.py" output="screen" />
    </group>

    <group ns="camera">
        <include file="$(find raspicam_node)/launch/camerav2_1280x960.launch" />
        <rosparam file="$(find line_follower)/config/rpi_camera_config.yaml" />
    </group>
</launch>
